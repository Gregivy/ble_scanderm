<?xml version='1.0' encoding='utf-8'?>
<widget id="Gregivy.blescanderm" version="0.1.0">
  <name>Ble Scanderm</name>
  <description>
    Example Tabris.js App
  </description>
  <author email="gpoi@mail.ru">
    John Smith
  </author>
  <preference name="EnableDeveloperConsole" value="$IS_DEBUG" />
  <plugin name="cordova-plugin-ble-central" source="npm" />
  <platform name="android">
    <edit-config file="app/src/main/AndroidManifest.xml" target="/manifest/application" mode="merge">
      <application android:usesCleartextTraffic="true" />
    </edit-config>
  </platform>
</widget>
